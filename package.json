{
  "name": "poke-freak",
  "version": "1.0.0",
  "description": "Let's pokemon battle.",
  "main": "poke-freak.js",
  "babel": {
    "presets": [
      "env"
    ],
    "env": {
      "test": {
        "plugins": [
          "istanbul"
        ]
      }
    }
  },
  "scripts": {
    "compile": "gulp compile",
    "compile-stub": "gulp compile-stub",
    "postcompile": "gulp webpack",
    "postcompile-stub": "gulp webpack",
    "pretest": "gulp ready-to-test",
    "test": "mocha cache/test -r setup-unit-test.js -R dot --require babel-register --exit --es_staging",
    "precoverage": "gulp ready-to-coverage",
    "coverage": "cross-env NODE_ENV=test nyc --all npm run test",
    "clean": "gulp clean"
  },
  "nyc": {
    "require": [
      "babel-register"
    ],
    "sourceMap": false,
    "instrument": false,
    "include": [
      "cache/test/**/*.js"
    ],
    "exclude": [
      "**/*-test.js",
      "**/mock-*.js"
    ],
    "reporter": [
      "lcov",
      "json"
    ]
  },
  "repository": {
    "type": "git",
    "url": "git+https://github.com/e-freak/poke-freak.git"
  },
  "author": "yuki",
  "license": "ISC",
  "bugs": {
    "url": "https://github.com/e-freak/poke-freak/issues"
  },
  "homepage": "https://github.com/e-freak/poke-freak#readme",
  "devDependencies": {
    "babel-core": "^6.25.0",
    "babel-plugin-istanbul": "^4.1.4",
    "babel-preset-env": "^1.6.1",
    "babel-register": "^6.26.0",
    "cross-env": "^5.1.3",
    "del": "^3.0.0",
    "gulp": "^3.9.1",
    "gulp-babel": "^7.0.1",
    "jsdom": "^11.6.2",
    "mocha": "^5.0.1",
    "nyc": "^11.5.0",
    "through2": "^2.0.3",
    "webpack-stream": "^4.0.2"
  }
}
